int main()
{
    char character1,character2;
    printf("Enter the two character:");
    scanf("%c %c",&character1,&character2);
    

    printf("Hello,ASCII value of character1 is %d\n:",character1);
    printf("Hello,ASCII value of character2 is %d\n:",character2);
    

    
    return 0;
}
